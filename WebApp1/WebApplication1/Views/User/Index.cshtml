@model IEnumerable<WebApplication1.Models.Authorization.AuthorizationUser>
@{
    ViewData["Title"] = "Users";
}
<head>
    <title>@ViewBag.Title</title>
</head>
<body>
    <table class="table table-bordered table-striped table-dark" >
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Manage</th>
        </tr>
        @foreach (var user in Model)
        {
    <tr>
        <td>@user.UserName</td>
        <td>@user.Email</td>
        <td>
            <a href="~/User/Update/@user.Id" class="btn btn-primary">Update</a>
            <a href="~/User/Delete/@user.Id" class="btn btn-danger">Delete</a>
        </td>
    </tr>
        }
    </table>
    <a href="~/User/Create" class="btn btn-light">Add</a>
</body>